#include<stdio.h>
#include<math.h>
void main()
{
    float x1,y1,x2,y2,distance;
    printf("enter  x1and y1 (coardinates of first point) ");
    scanf("%f %f\n",&x1, &y1);
    printf("enter x2 and y2(cordinate of second point)");
    scanf("%f %f\n",&x2,&y2);
    distance=sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
    printf("the distance between the two points is:%2f\n",distance);
}
